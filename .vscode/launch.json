{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Tweek-api",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "api-build",
            "program": "${workspaceRoot}/services/api/Tweek.ApiService.NetCore/bin/Debug/netcoreapp1.1/Tweek.ApiService.NetCore.dll",
            "args": [],
            "cwd": "${workspaceRoot}/services/api/Tweek.ApiService.NetCore",
            "stopAtEntry": false,
            "launchBrowser": {
                "enabled": true,
                "args": "${auto-detect-url}",
                "windows": {
                    "command": "cmd.exe",
                    "args": "/C start ${auto-detect-url}/status"
                },
                "osx": {
                    "command": "open"
                },
                "linux": {
                    "command": "xdg-open"
                }
            },
            "env": {
                "RulesBlob.Url": "http://localhost:4002/ruleset/latest",
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceRoot}/Views"
            }
        },
        {
            "name": "Tweek-analytics-api",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "analytics-api-build",
            "program": "${workspaceRoot}/services/analytics-api/Tweek.AnalyticsApiService.NetCore/bin/Debug/netcoreapp1.1/Tweek.AnalyticsApiService.NetCore.dll",
            "args": [],
            "cwd": "${workspaceRoot}/services/analytics-api/Tweek.AnalyticsApiService.NetCore",
            "stopAtEntry": false,
            "launchBrowser": {
                "enabled": true,
                "args": "${auto-detect-url}",
                "windows": {
                    "command": "cmd.exe",
                    "args": "/C start ${auto-detect-url}/status"
                },
                "osx": {
                    "command": "open"
                },
                "linux": {
                    "command": "xdg-open"
                }
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        },
        {
			"name": "Editor",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/services/editor/node_modules/react-project/bin/react-project.js",
			"stopOnEntry": false,
			"args": [
				"start"
			],
			"cwd": "${workspaceRoot}/services/editor",
			"preLaunchTask": "dev-env-editor",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development",
				"AUTO_RELOAD": "hot",
                "GIT_USER":"git",
                "GIT_URL":"ssh://git@localhost:4001/tweek/repo",
                "GIT_PUBLIC_KEY_PATH": "../git-service/ssh/tweekgit.pub",
                "GIT_PRIVATE_KEY_PATH": "../git-service/ssh/tweekgit",
                "TWEEK_API_HOSTNAME":"http://localhost:4003"
			},
			"externalConsole": false,
			"sourceMaps": false,
			"outDir": null
		},
        {
			"name": "Editor (tests)",
			"type": "node",
			"request": "launch",
			"stopOnEntry": false,
            "program": "${workspaceRoot}/services/editor/node_modules/jest-cli/bin/jest.js",
			"cwd": "${workspaceRoot}/services/editor",
            "args": ["--runInBand"],
            "runtimeArgs": [
                "--nolazy"
            ],
			"env": {
				"NODE_ENV": "development",
				"AUTO_RELOAD": "hot",
                "GIT_USER":"git",
                "GIT_URL":"ssh://git@localhost:40/tweek/repo",
                "GIT_PUBLIC_KEY_PATH": "../git-service/ssh/tweekgit.pub",
                "GIT_PRIVATE_KEY_PATH": "../git-service/ssh/tweekgit",
                "TWEEK_API_HOSTNAME":"http://localhost:4003"
			},
			"externalConsole": false,
			"sourceMaps": false,
			"outDir": null
		},
        {
			"name": "E2E (tests)",
			"type": "node",
			"request": "launch",
			"stopOnEntry": false,
            "program": "${workspaceRoot}/e2e/node_modules/webdriverio/bin/wdio",
			"cwd": "${workspaceRoot}/e2e",
            "args": ["wdio.conf.js"],
            "runtimeArgs": [
                "--nolazy"
            ],
			"preLaunchTask": "dev-env-e2e",
			"env": {
                "BACKOFFICE_URL": "http://localhost:4004/"
			},
			"externalConsole": false,
			"sourceMaps": false,
			"outDir": null
		}
    ]
}