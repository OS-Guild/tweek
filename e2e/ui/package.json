{
  "name": "e2e-tests",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=8.1.0"
  },
  "scripts": {
    "start": "yarn run docker-compose up -- --force-recreate -d editor",
    "test": "wdio wdio.conf.js",
    "rebuild": "yarn run docker-compose build",
    "test:full-env": "yarn run start && yarn run test",
    "test:full-env:rebuild": "yarn run rebuild && yarn run test:full-env",
    "test:docker": "yarn run start && yarn run rebuild e2e-ui && yarn run docker-compose run -- --rm e2e-ui",
    "test:docker:rebuild": "yarn run rebuild && yarn run test:docker",
    "teardown": "yarn run docker-compose down -- --remove-orphans",
    "logs": "yarn run docker-compose logs --",
    "docker-compose": "docker-compose -f ../../deployments/dev/docker-compose.yml -f ../../deployments/dev/docker-compose.override.yml -f ../../deployments/dev/docker-compose.e2e.yml",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "gitDir": "../../",
    "linters": {
      "**/e2e/ui/**/*.js": [
        "prettier --print-width 100 --single-quote --trailing-comma all --no-bracket-spacing=false --write",
        "git add"
      ]
    }
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babel-cli": "6.24.1",
    "babel-core": "^6.18.2",
    "babel-eslint": "7.2.3",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "chai": "4.1.0",
    "chai-string": "^1.4.0",
    "chance": "1.0.10",
    "jsonwebtoken": "^7.4.0",
    "mocha": "^3.4.2",
    "moment": "2.18.1",
    "nconf": "^0.8.4",
    "ramda": "^0.24.0",
    "rxjs": "^5.4.2",
    "supertest": "^3.0.0",
    "wdio-mocha-framework": "0.5.10",
    "wdio-spec-reporter": "0.1.0",
    "webdriverio": "4.8.0"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^4.0.3",
    "prettier": "^1.5.3",
    "wdio-selenium-standalone-service": "0.0.9"
  }
}
