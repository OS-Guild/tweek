  "engines": {
    "node": ">=8.7.0"
  },
  "scripts": {
    "start": "yarn docker-compose up --force-recreate -d git redis && yarn docker-compose up -d editor management",
    "test": "wdio wdio.conf.js",
    "build": "yarn docker-compose build",
    "test:full-env": "yarn start && yarn test",
    "test:full-env:build": "yarn build && yarn test:full-env",
    "test:docker": "yarn start && yarn build e2e-ui && yarn docker-compose run --rm e2e-ui",
    "test:docker:build": "yarn build && yarn test:docker",
    "teardown": "yarn docker-compose down --remove-orphans",
    "logs": "yarn docker-compose logs",
    "docker-compose": "docker-compose -f ../../deployments/dev/docker-compose.yml -f ../../deployments/dev/docker-compose.override.yml -f ../../deployments/dev/docker-compose.e2e.yml"
  },
  "dependencies": {
    "axios": "^0.17.0",
    "babel-cli": "6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "8.0.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "chai": "4.1.2",
    "chai-string": "^1.4.0",
    "chance": "1.0.12",
    "jsonwebtoken": "^8.1.0",
    "mocha": "^4.0.1",
    "moment": "2.19.1",
    "nconf": "^0.8.5",
    "ramda": "^0.25.0",
    "rxjs": "^5.5.2",
    "supertest": "^3.0.0",
    "wdio-mocha-framework": "0.5.11",
