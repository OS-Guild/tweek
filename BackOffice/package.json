{
  "name": "backoffice",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "engines": {
    "node": ">=6.9.0"
  },
  "react-project": {
    "server": "modules/server.js",
    "client": "modules/client.js",
    "webpack": "webpack.config.js"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run react-project:start:prod || npm run react-project:start:dev",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:integration": "mocha --compilers js:babel-register ./integration-tests/**/*.js",
    "test:e2e": "./node_modules/.bin/wdio wdio.conf.js",
    "coverage:upload:code-climate": "codeclimate-test-reporter < ./coverage-unit/lcov.info",
    "react-project:start:dev": "node ./node_modules/react-project/bin/react-project.js start",
    "react-project:start:prod": "rm -rf .build && react-project build && node .build/server.js",
    "build-prod": "rm -rf .build && react-project build"
  },
  "author": "",
  "jest": {
    "testRegex": "/spec/unit/.*",
    "automock": false,
    "modulePathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^.*css$": "empty/object"
    },
    "coverageDirectory": "./coverage-unit/",
    "collectCoverage": true,
    "unmockedModulePathPatterns": [
      "/node_modules/react",
      "node_modules/react-dom",
      "node_modules/react-addons-test-utils",
      "node_modules/recompose",
      "node_modules/ramda",
      "node_modules/chai",
      "node_modules/chai-as-promise",
      "node_modules/fetch-mock",
      "babel-runtime",
      "spec/unit/.*"
    ]
  },
  "license": "ISC",
