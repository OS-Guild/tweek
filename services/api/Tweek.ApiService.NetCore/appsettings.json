{
    "Rules": {
      "Management": {
        "Url": "http://management.playground.tweek.host"
      }
    },
    "Addons": {
        "PingIdentity": {
            "className": "Tweek.Addons.Auth.PingIdentity.PingIdentityAuth",
            "assemblyName": "Tweek.Addons.Auth.PingIdentity"
        },
        "TweekLegacySupport":{
            "className": "Tweek.ApiService.NetCore.Security.TweekLegacySupport",
            "assemblyName": "Tweek.ApiService.NetCore"
        },
        "RulesManagement": {
            "className": "Tweek.Drivers.Rules.Management.RulesManagementAddon",
            "assemblyName": "Tweek.Drivers.Rules.Management"
        }
    },
    "Logging": {
        "IncludeScopes": false,
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "AspNetMetrics": {
      "MetricsTextEndpointEnabled": true,
      "HealthEndpointEnabled": true,
      "MetricsEndpointEnabled": true,
      "PingEndpointEnabled": true,
      "OAuth2TrackingEnabled": true,
      "HealthEndpoint": "/health",
      "MetricsEndpoint": "/metrics",
      "MetricsTextEndpoint": "/metrics-text",
      "PingEndpoint": "/ping"
    },
    "PUBLIC_KEY_PATH": "../../git-service/ssh/tweekgit_public.pfx"
}
