{
  "name": "backoffice",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "engines": {
    "node": ">=6.9.0"
  },
  "scripts": {
    "start:service-worker": "webpack --config ./service-worker/webpack.config.dev.js --watch",
    "start:client": "react-scripts start",
    "start:server": "nodemon --watch server server/index.js --exec babel-node -- --GIT_USER git --GIT_URL ssh://git@localhost:4001/tweek/repo --GIT_PUBLIC_KEY_PATH ../git-service/ssh/tweekgit.pub --GIT_PRIVATE_KEY_PATH ../git-service/ssh/tweekgit --TWEEK_API_HOSTNAME http://localhost:4003",
    "start": "concurrently --kill-others \"yarn run start:service-worker\" \"yarn run start:client\" \"yarn run start:server\"",
    "start:full-env": "npm run docker-compose -- up -d tweek-api && npm run start",
    "build:service-worker": "webpack --config ./service-worker/webpack.config.prod.js",
    "build:client": "react-scripts build",
    "build:server": "babel server --out-dir dist",
    "build": "concurrently --kill-others-on-fail \"yarn run build:service-worker && yarn run build:client\" \"yarn run build:server\"",
    "test": "jest",
    "eject": "react-scripts eject",
    "teardown": "npm run docker-compose -- down",
    "docker-compose": "docker-compose -f ../../deployments/dev/docker-compose.yml -f ../../deployments/dev/docker-compose.override.yml",
    "format": "prettier-eslint --trailing-comma all --print-width 100 --single-quote --write",
    "format:all": "npm run format \"**/*.js\"",
    "precommit": "lint-staged"
  },
  "author": "",
  "proxy": {
    "/api": {
      "target": "http://localhost:3001",
      "secure": false
    },
    "/socket.io": {
      "target": "http://localhost:3001",
      "secure": false,
      "ws": true
    },
    "/sockjs-node": {
      "target": "http://localhost:3001",
      "secure": false
    },
    "/login": {
      "target": "http://localhost:3001",
      "secure": false
    }
  },
  "lint-staged": {
    "gitDir": "../../",
    "linters": {
      "**/editor/**/*.js": [
        "npm run format",
        "git add"
      ]
    }
  },
  "jest": {
    "testRegex": "/spec/unit/.*",
    "automock": false,
    "modulePathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^.*css$": "empty/object"
    },
    "coverageDirectory": "./coverage-unit/",
    "collectCoverage": true,
    "unmockedModulePathPatterns": [
      "/node_modules/react",
      "node_modules/react-dom",
      "node_modules/react-addons-test-utils",
      "node_modules/recompose",
      "node_modules/ramda",
      "node_modules/chai",
      "node_modules/chai-as-promise",
      "node_modules/fetch-mock",
      "babel-runtime",
      "spec/unit/.*"
    ]
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^0.16.1",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "chance": "^1.0.9",
    "change-case": "^3.0.1",
    "classnames": "^2.2.5",
    "crypto": "^0.0.3",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "fs-extra": "^3.0.1",
    "glob": "^7.1.2",
    "glob-promise": "^3.1.0",
    "highland": "^2.10.5",
    "history": "^4.6.1",
    "jsonwebtoken": "^7.4.1",
    "lock-queue": "^1.0.1",
    "lunr": "^2.0.4",
    "moment": "^2.18.1",
    "nconf": "^0.8.4",
    "nodegit": "0.14.1",
    "passport": "^0.3.2",
    "passport-azure-ad": "^3.0.6",
    "promisify-node": "^0.4.0",
    "prop-types": "^15.5.10",
    "ramda": "^0.24.1",
    "react": "^15.5.4",
    "react-autosize-textarea": "^0.4.3",
    "react-dom": "^15.5.4",
    "react-draggable": "^2.2.6",
    "react-highlight-words": "^0.7.2",
    "react-json": "^0.2.1",
    "react-monaco-editor": "^0.8.1",
    "react-notification-system": "^0.2.14",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "next",
    "react-sanfona": "^0.2.1",
    "react-tabs": "^1.0.0",
    "react-tag-input": "^4.7.2",
    "react-tap-event-plugin": "^2.0.1",
    "react-title-component": "^1.0.1",
    "react-tooltip": "^3.3.0",
    "react-virtualized": "^9.7.6",
    "recompose": "^0.23.4",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.3",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "rodal": "^1.5.1",
    "rxjs": "^5.4.0",
    "socket.io": "^2.0.2",
    "velocity-react": "^1.3.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.0.1",
    "chai-things": "^0.2.0",
    "concurrently": "^3.4.0",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "fetch-mock": "^5.11.0",
    "husky": "^0.13.4",
    "jest-cli": "^20.0.4",
    "jest-css-modules": "^1.1.0",
    "lint-staged": "^3.6.0",
    "nodemon": "^1.11.0",
    "prettier-eslint-cli": "^4.0.2",
    "react-scripts": "1.0.7",
    "rimraf": "^2.6.1"
  }
}
