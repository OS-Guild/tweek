{
  "name": "backoffice",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "engines": {
    "node": ">=6.9.0"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "nodemon --watch modules/api -e js  --exec npm run cli -- start",
    "start:prod": "npm run build-prod && node .build/server.js",
    "start:dev:full-env": "docker-compose -f ../../deployments/dev/docker-compose.yml up -d tweek-api && npm run start:dev -- --NODE_ENV development --AUTO_RELOAD hot --GIT_USER git --GIT_URL ssh://git@localhost:4001/tweek/repo --GIT_PUBLIC_KEY_PATH ../git-service/ssh/tweekgit.pub --GIT_PRIVATE_KEY_PATH ../git-service/ssh/tweekgit --TWEEK_API_HOSTNAME http://localhost:4003",
    "pull-env": "docker-compose -f ../../deployments/dev/docker-compose.yml pull",
    "build-env": "docker-compose -f ../../deployments/dev/docker-compose.yml build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "mocha --compilers js:babel-register ./integration-tests/**/*.js",
    "coverage:upload:code-climate": "codeclimate-test-reporter < ./coverage-unit/lcov.info",
    "build-prod": "rm -rf .build && npm run cli -- build --NODE_ENV production",
    "cli": "babel scripts/src --out-dir scripts/lib && node ./scripts/lib/cli.js"
  },
  "author": "",
  "jest": {
    "testRegex": "/spec/unit/.*",
    "automock": false,
    "modulePathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^.*css$": "empty/object"
    },
    "coverageDirectory": "./coverage-unit/",
    "collectCoverage": true,
    "unmockedModulePathPatterns": [
      "/node_modules/react",
      "node_modules/react-dom",
      "node_modules/react-addons-test-utils",
      "node_modules/recompose",
      "node_modules/ramda",
      "node_modules/chai",
      "node_modules/chai-as-promise",
      "node_modules/fetch-mock",
      "babel-runtime",
      "spec/unit/.*"
    ]
  },
  "license": "ISC",
  "dependencies": {
    "axios": "0.16.0",
    "bluebird": "3.4.6",
    "body-parser": "1.15.0",
    "chance": "1.0.3",
    "change-case": "3.0.1",
    "classnames": "2.2.5",
    "compression": "1.6.1",
    "deep-diff": "0.3.4",
    "express": "4.13.4",
    "express-session": "1.14.0",
    "fs-extra": "0.30.0",
    "glob-promise": "^2.0.0",
    "helmet": "1.1.0",
