    "express": "4.16.3",
    "fast-json-patch": "^2.0.6",
    "history": "^4.7.2",
    "idb-keyval": "^2.5.1",
    "jwt-decode": "2.2.0",
    "lunr": "^2.1.6",
    "md5": "2.2.1",
    "moment": "^2.22.0",
    "oidc-client": "1.5.4",
    "papp-polyfill": "^1.1.0",
    "passport": "^0.4.0",
    "passport-azure-ad": "^4.0.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-http": "^0.3.0",
    "passport-oauth2": "^1.4.0",
    "prop-types": "^15.6.1",
    "query-string": "5.1.1",
    "ramda": "^0.25.0",
    "react": "16.7.0",
    "react-autosize-textarea": "^1.0.2",
    "react-calendar": "2.17.4",
    "react-document-title": "2.0.3",
    "react-dom": "16.7.0",
