    "passport": "^0.4.0",
    "passport-azure-ad": "^3.0.12",
    "passport-google-oauth20": "^1.0.0",
    "passport-http": "^0.3.0",
    "passport-oauth2": "^1.4.0",
    "prop-types": "^15.6.1",
    "query-string": "5.1.0",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-autosize-textarea": "^1.0.2",
    "react-document-title": "2.0.3",
