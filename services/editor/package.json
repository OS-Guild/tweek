{
  "name": "tweek-editor",
  "version": "0.3.3",
  "main": "dist/index.js",
  "repository": "Soluto/tweek",
  "author": "Soluto",
  "license": "MIT",
  "engines": {
    "node": ">=8.7.0"
  },
  "scripts": {
    "start:service-worker": "webpack --config ./service-worker/webpack.config.js --watch",
    "start:client": "yarn build-less && concurrently --kill-others \"yarn watch-less\" \"react-app-rewired start\"",
    "start:server": "nodemon --watch server server/index.js --exec babel-node -- --GIT_USER git --GIT_URL ssh://git@localhost:4001/tweek/repo --GIT_PUBLIC_KEY_PATH ../../deployments/dev/ssh/tweekgit.pub --GIT_PRIVATE_KEY_PATH ../../deployments/dev/ssh/tweekgit --TWEEK_API_HOSTNAME http://api.localtest.me --AUTHORING_API_HOSTNAME http://authoring.localtest.me --PUBLISHING_HOSTNAME=http://localhost:4010",
    "start:full-env": "yarn docker-compose up -d gateway publishing && yarn start",
    "start:release": "node ./dist/index.js",
    "build:service-worker": "webpack --config ./service-worker/webpack.config.js",
    "build:client": "yarn build-less && react-app-rewired build",
    "build:server": "babel server --out-dir dist && exit 0",
    "build": "concurrently --kill-others-on-fail \"yarn build:service-worker && yarn build:client\" \"yarn build:server\"",
    "test": "jest",
    "eject": "react-scripts eject",
    "teardown": "yarn docker-compose down --remove-orphans",
    "logs": "yarn docker-compose logs",
    "docker-compose": "docker-compose -f ../../deployments/dev/docker-compose.yml -f ../../deployments/dev/docker-compose.override.yml",
    "build-less": "node-less-chokidar src/",
    "watch-less": "yarn build-less && node-less-chokidar src/ -o src/ --watch --recursive"
  },  
  "jest": {
    "testRegex": "/spec/unit/.*\\.js$",
    "coverageDirectory": "./coverage-unit/",
    "collectCoverage": true
  },
  "dependencies": {
    "animakit-expander": "^2.1.4",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "chance": "^1.0.13",
    "change-case": "^3.0.2",
    "classnames": "^2.2.5",
    "crypto": "^1.0.1",
    "emotion": "9.0.2",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "fast-json-patch": "^2.0.6",
    "helmet": "^3.12.0",
    "history": "^4.7.2",
    "idb-keyval": "^2.3.0",
    "jsonwebtoken": "^8.2.0",
    "lunr": "^2.1.6",
