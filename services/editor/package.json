    "morgan": "1.7.0",
    "nconf": "0.8.4",
    "node-fetch": "1.3.3",
    "nodegit": "0.14.1",
    "passport-azure-ad": "^3.0.4",
    "promisify-node": "0.4.0",
    "ramda": "0.23.0",
    "react": "15.4.2",
    "react-addons-shallow-compare": "15.4.2",
    "react-autosize-textarea": "0.3.1",
    "react-bootstrap-typeahead": "1.2.0",
    "react-dom": "15.4.2",
