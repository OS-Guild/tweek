    "react-title-component": "1.0.1",
    "react-tooltip": "^3.2.2",
    "react-virtualized": "8.11.4",
    "recompose": "0.20.0",
    "redux": "3.4.0",
    "redux-actions": "0.9.1",
    "redux-promise": "0.5.3",
    "redux-thunk": "2.0.1",
    "rimraf": "2.5.2",
    "rodal": "1.4.1",
    "rxjs": "5.0.0-beta.10",
    "rxjs-es": "5.0.0-beta.10",
