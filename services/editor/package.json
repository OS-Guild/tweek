    "fetch-mock": "^6.0.1",
    "jest-cli": "^22.4.2",
    "jest-css-modules": "^1.1.0",
    "node-less-chokidar": "^0.1.2",
    "nodemon": "^1.17.1",
    "react-app-rewired": "1.4.1",
    "react-scripts": "1.1.1",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2"
  }
}
