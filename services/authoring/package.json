{
  "name": "tweek-authoring",
  "version": "1.0.0-rc7",
  "main": "src/server.js",
  "repository": "Soluto/tweek",
  "author": "Soluto",
  "license": "MIT",
  "engines": {
    "node": ">=10.15.0"
  },
  "scripts": {
    "test": "mocha -r ts-node/register test/**/*.ts --exit",
    "build": "yarn run clean && tsc && cpr ./src/search-index/build/ ./dist/search-index/build/ -o",
    "clean": "rimraf dist",
    "start": "ts-node ./src/server.ts",
    "start:release": "node ./dist/server.js",
    "start:debug": "node --inspect=5858 ./dist/server.js",
    "start:dev": "cross-env GIT_SSH=../ssh-helper nodemon --watch src --ext ts --exec yarn -- start --GIT_USER git --GIT_URL ssh://git@localhost:4001/tweek/repo --GIT_PUBLIC_KEY_PATH ../../deployments/dev/ssh/tweekgit.pub --GIT_PRIVATE_KEY_PATH ../../deployments/dev/ssh/tweekgit",
    "start:full-env": "yarn docker-compose up -d api publishing git && yarn start:dev",
    "logs": "yarn docker-compose logs",
    "docker-compose": "docker-compose -f ../../deployments/dev/docker-compose.yml -f ../../deployments/dev/docker-compose.override.yml",
    "teardown": "yarn docker-compose down --remove-orphans"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "bunyan": "^1.8.12",
    "express": "^4.16.4",
    "fast-json-patch": "^2.0.6",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.2",
    "highland": "^2.11.1",
    "jszip": "^3.1.5",
    "lock-queue": "^1.0.1",
    "lunr": "^2.1.5",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "nconf": "^0.10.0",
    "node-getopt": "^0.3.2",
    "nodegit": "^0.24.1",
    "object-hash": "^1.3.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-strategy": "^1.0.0",
    "ramda": "^0.26.1",
    "rxjs": "^6.4.0",
    "shortid": "^2.2.14",
    "simple-git": "^1.84.0",
    "sshpk": "^1.14.1",
    "typescript-ioc": "^1.0.2",
    "typescript-rest": "^2.0.0",
    "typescript-rest-swagger": "^0.0.24",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.17",
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.0.4",
    "@types/express": "^4.16.1",
    "@types/fs-extra": "^5.0.5",
    "@types/mocha": "^5.2.6",
    "@types/morgan": "^1.7.35",
    "@types/nconf": "^0.10.0",
    "@types/node": "^11.13.0",
    "@types/nodegit": "^0.24.6",
    "@types/object-hash": "^1.3.0",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.0",
    "@types/passport-strategy": "^0.2.32",
    "@types/ramda": "^0.26.6",
    "@types/shortid": "^0.0.29",
    "@types/sinon": "^7.0.13",
    "@types/sshpk": "^1.10.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-things": "^0.2.0",
    "cpr": "^3.0.1",
    "cross-env": "^5.2.0",
    "mocha": "^6.0.2",
    "nodemon": "^1.18.10",
    "rimraf": "^2.6.3",
    "simple-mock": "^0.8.0",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.15.0",
    "typescript": "^3.4.1"
  }
}
