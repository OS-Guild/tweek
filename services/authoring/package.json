{
  "name": "tweek-authoring",
  "version": "1.0.0-rc14",
  "main": "src/server.js",
  "repository": "Soluto/tweek",
  "author": "Soluto",
  "license": "MIT",
  "engines": {
    "node": ">=10.15.0"
  },
  "scripts": {
    "test": "mocha -r ts-node/register test/**/*.spec.ts --exit",
    "build": "yarn run clean && tsc && cpr ./src/search-index/build/ ./dist/search-index/build/ -o",
    "clean": "rimraf dist",
    "start": "ts-node ./src/server.ts",
    "start:release": "node ./dist/server.js",
    "start:debug": "node --inspect=5858 ./dist/server.js",
    "start:dev": "cross-env GIT_SSH=../ssh-helper nodemon --watch src --ext ts --exec yarn -- start --GIT_USER git --GIT_URL ssh://git@localhost:4001/tweek/repo --GIT_PUBLIC_KEY_PATH ../../deployments/dev/ssh/tweekgit.pub --GIT_PRIVATE_KEY_PATH ../../deployments/dev/ssh/tweekgit",
    "start:full-env": "yarn docker-compose up -d api publishing git && yarn start:dev",
    "logs": "yarn docker-compose logs",
    "docker-compose": "docker-compose -f ../../deployments/dev/docker-compose.yml -f ../../deployments/dev/docker-compose.override.yml",
    "teardown": "yarn docker-compose down --remove-orphans",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write **/*.ts"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.25.24",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "bluebird": "^3.7.2",
    "bunyan": "^1.8.15",
    "express": "^4.17.3",
    "fast-json-patch": "^3.1.1",
    "fs-extra": "^11.1.0",
    "glob": "^9.2.1",
    "highland": "^2.13.5",
    "jszip": "^3.10.1",
    "lock-queue": "^1.0.1",
    "lunr": "^2.3.9",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "nconf": "^0.12.0",
    "node-getopt": "^0.3.2",
    "nodegit": "^0.28.0-alpha.21",
    "object-hash": "^3.0.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-strategy": "^1.0.0",
    "ramda": "^0.28.0",
    "rxjs": "^7.8.0",
    "shortid": "^2.2.16",
    "simple-git": "^3.17.0",
    "sshpk": "^1.17.0",
    "typescript-ioc": "^3.2.2",
    "typescript-rest": "^3.0.4",
    "typescript-rest-ioc": "^1.0.1",
    "typescript-rest-swagger": "^1.1.7",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.38",
    "@types/bunyan": "^1.8.8",
    "@types/chai": "^4.3.4",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/highland": "^2.12.14",
    "@types/lunr": "^2.3.4",
    "@types/mocha": "^10.0.1",
    "@types/morgan": "^1.9.4",
    "@types/nconf": "^0.10.3",
    "@types/node": "^18.15.0",
    "@types/nodegit": "0.28.3",
    "@types/object-hash": "^3.0.2",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-strategy": "^0.2.35",
    "@types/ramda": "^0.28.23",
    "@types/shortid": "^0.0.29",
    "@types/sinon": "^10.0.13",
    "@types/sshpk": "^1.17.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "chai-things": "^0.2.0",
    "cpr": "^3.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.8.4",
    "rimraf": "^4.4.0",
    "simple-mock": "^0.8.0",
    "sinon": "^15.0.1",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "yarn-audit-fix": "^9.3.9"
  }
}
