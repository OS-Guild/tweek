  "dependencies": {
    "axios": "^0.16.1",
    "bluebird": "^3.5.0",
    "change-case": "^3.0.1",
    "crypto": "^0.0.3",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "fs-extra": "^3.0.1",
    "glob": "^7.1.2",
    "glob-promise": "^3.1.0",
    "highland": "^2.10.5",
    "jsonwebtoken": "^7.4.1",
    "lock-queue": "^1.0.1",
    "lunr": "^2.0.4",
    "nconf": "^0.8.4",
    "nodegit": "^0.18.3",
    "passport": "^0.3.2",
    "passport-azure-ad": "^3.0.6",
